<AnchorPane prefHeight="345.0" prefWidth="594.0" style="-fx-background-color: #003049;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.BtnScanReceiptController">
